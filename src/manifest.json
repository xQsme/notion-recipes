{
	"manifest_version": 2,
	"name": "Notion BBC Good Food Recipe Scraper",
	"version": "1.0",
	"description": "Parses recipes from BBC Good Food and creates a page for them in Notion.",
	"icons": {
		"48": "icons/notion.png"
	},
	"browser_action": {
		"default_icon": {
			"48": "icons/notion.png"
		},
		"default_title": "Save Recipe!"
	},
	"background": {
		"scripts": [
			"background.ts"
		]
	},
	"permissions": [
		"activeTab",
		"*://*.bbcgoodfood.com/*"
	]
}